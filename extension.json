{
    "name": "MyMWExtension",
    "author": "Blas",
    "license-name": "GPL-2.0+",
    "version": "0.0.1",
    "namemsg": "myMWExtension",
    "descriptionmsg": "myMWExtension-desc",
    "manifest_version": 1,
    "MessagesDirs": {
        "MyMWExtension": [ "i18n" ]
    },
    "AutoloadClasses": {
        "SpecialMyMWExtension": "specials/SpecialMyMWExtension.php",
        "MyMWExtensionHooks": "includes/MyMWExtensionHooks.php",
        "GamLog": "includes/GamLog.php",
        "WebServer": "includes/WebServer.php",
        "UserModel": "includes/model/UserModel.php"
    },
    "Hooks": {
        "UserLoginComplete": "MyMWExtensionHooks::onUserLoginComplete",
        "LoadExtensionSchemaUpdates": "MyMWExtensionHooks::onLoadExtensionSchemaUpdates",
        "LocalUserCreated": "MyMWExtensionHooks::onLocalUserCreated",
        "ParserFirstCallInit": "MyMWExtensionHooks::onParserFirstCallInit",
        "PageContentSaveComplete": "MyMWExtensionHooks::onPageContentSaveComplete"
    },
    "SpecialPages": {
        "MyMWExtension": "SpecialMyMWExtension"
    },
    "ExtensionMessagesFiles": {
        "MyMWExtensionAlias": "MyMWExtension.alias.php",
        "ExampleExtensionMagic": "includes/ExampleExtension.i18n.php"
    },
    "type": "other",
    "config": {
        "ArduinoWerbServerOn": false,
        "ArduinoWebServerIP": "127.0.0.1"
    },
    "ResourceModules": {
        "ext.myMWExtension": {
            "scripts": [
                "ext.myMWExtension.core.js"
            ]
        }
    },
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "MyMWExtension/modules"
	}
}
